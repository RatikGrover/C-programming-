//type casting using ascii value
#include<stdio.h>
int main()
{
    char ch;
    printf("\nEnter a single character : ");
    scanf("%c",&ch);
    printf("\nEntered character is : %c",&ch);
    if (ch>=65&&ch<=90)
        printf("\nIt is an upper case alphabet ");
    else if (ch>=90&&ch<=122)
        printf("\nIt is an lower case alphabet ");
    else if(ch>=48&&ch<=57)
        printf("\nIt is a digit ");\
    else
        printf("\nIts a special symbol ");
    printf("\n\n");
    return 0;
}
